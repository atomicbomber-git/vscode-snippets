{
    "Bootstrap Form Group Basic Input": {
        "prefix": "vue-bs-input",
        "body": [
            "<div class='form-group'>",
            "    <label for='${1:name}'> ${2:label}: </label>",
            "    <input",
            "        v-model='${1:name}'",
            "        class='form-control'",
            "        :class='{'is-invalid': get(this.error_data, 'errors.${1:name}[0]', false)}'",
            "        type='text' id='${1:name}' placeholder='${2:label}'>",
            "    <div class='invalid-feedback'>{{ get(this.error_data, 'errors.${1:name}[0]', false) }}</div>",
            "</div>",
        ]
    },

    "Vue2 Google Maps GmapMap": {
        "prefix": "vue-gmapmap",
        "body": [
            "<GmapMap",
            "    :center=\"{lat: ${1:lat}, lng: ${2:lng}}\"",
            "    :zoom=\"${3:zoom}\"",
            "    map-type-id=\"${4:terrain}\"",
            "    style=\"height: ${5:300px}; width: ${6:300px}\">",
            "</GmapMap>",
        ]
    },
}