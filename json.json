{
    "VS Code Snippet": {
        "prefix": "vscode-snippet",
        "body": [
            "\"${1:Snippet Name}\": {",
            "    \"prefix\": \"${2:snippet-prefix}\",",
            "    \"body\": [",
            "        \"${3:snippet-body}\",",
            "    ]",
            "},",
        ]
    },

    "Composer PSR-4 Autoloading Setting": {
        "prefix": "psr-4-autoload",
        "body": [
            "\"autoload\": {",
            "    \"psr-4\": {",
            "        \"App\\\\\\\\\": \"app/\"",
            "    }",
            "}",
        ]
    },
}