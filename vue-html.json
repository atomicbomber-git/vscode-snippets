{
    "Vue Cleave": {
        "prefix": "vue-cleave",
        "body": [
            "<vue-cleave",
            "    class=\"form-control form-control-sm\"",
            "    v-model.number=\"${1:model}\"",
            "    :options=\"{ numeral: true, numeralDecimalMark: ',', delimiter: '.' }\">",
            "</vue-cleave>"
        ]
    },

    "Bootstrap Form Submit Button": {
        "prefix": "bs-form-submit-button",
        "body": [
           "<div class=\"d-flex justify-content-end\">",
           "    <button class=\"btn btn-primary\">",
           "        Tambahkan ${1:Entity} Baru",
           "    </button>",
           "</div>",
        ]
    },
   

    "Vue Multiselect": {
        "prefix": "vue-multiselect",
        "body": [
            "<multiselect",
            "    placeholder=\"${4:Placeholder}\"",
            "    :allow-empty=\"false\"",
            "    selectLabel=\"\"",
            "    selectedLabel=\"\"",
            "    deselectLabel=\"\"",
            "    track-by=\"${1:id}\"",
            "    label=\"${2:label}\"",
            "    :options=\"${3:item}s\"",
            "    v-model=\"${3:item}\"",
            "    :preselect-first=\"true\"",
            "></multiselect>",
        ]
    },

    "Bulma Sidebar": {
        "prefix": "bulma-sidebar",
        "body": [
            "<aside class=\"menu\">",
            "    <p class=\"menu-label\">",
            "        General",
            "    </p>",
            "    <ul class=\"menu-list\">",
            "        <li><a>Dashboard</a></li>",
            "        <li><a>Customers</a></li>",
            "    </ul>",
            "    <p class=\"menu-label\">",
            "        Administration",
            "    </p>",
            "    <ul class=\"menu-list\">",
            "        <li><a>Team Settings</a></li>",
            "        <li>",
            "            <a class=\"is-active\">Manage Your Team</a>",
            "            <ul>",
            "                <li><a>Members</a></li>",
            "                <li><a>Plugins</a></li>",
            "                <li><a>Add a member</a></li>",
            "            </ul>",
            "        </li>",
            "        <li><a>Invitations</a></li>",
            "        <li><a>Cloud Storage Environment Settings</a></li>",
            "        <li><a>Authentication</a></li>",
            "    </ul>",
            "    <p class=\"menu-label\">",
            "        Transactions",
            "    </p>",
            "    <ul class=\"menu-list\">",
            "        <li><a>Payments</a></li>",
            "        <li><a>Transfers</a></li>",
            "        <li><a>Balance</a></li>",
            "    </ul>",
            "</aside>",
        ]
    },

    "Milestone Bootstrap Form Input": {
        "prefix": "milestone-form-input",
        "body": [
            "<fieldset class='form-group'>",
            "    <label for='${1:field}'> ${Label}: </label>",
            "    <input",
            "        v-model='${1:field}'",
            "        class='form-control'",
            "        :class=\"{'error': get(this.error_data, 'errors.${1:field}[0]', false)}\"",
            "        type='${3:password}' id='${1:field}' placeholder='${Label}'>",
            "    <div",
            "        v-if=\"get(this.error_data, 'errors.${1:field}[0]', false)\"",
            "        class='error'>                    ",
            "        {{ get(this.error_data, 'errors.${1:field}[0]', false) }}",
            "    </div>",
            "</fieldset>",
        ]
    },

    "Bulma Input": {
        "prefix": "bulma-input",
        "body": [
            "<div class=\"field\">",
            "    <label for=\"${1:field}\" class=\"label\"> ${2:label}: </label>",
            "    <div class=\"control\">",
            "        <input placeholder=\"${2:label}\" :value=\"${1:field}\" type=\"${3:text}\" name=\"${1:field}\"",
            "             class=\"input\"",
            "             :class=\"{'is-error': get(this.error_data, 'errors.field[0]', false)}\"",
            "            />",
            "    </div>",
            "    <p v-if=\"get(this.error_data, 'errors.${1:field}[0]', false)\" class=\"help is-danger\">",
            "        {{ get(this.error_data, 'errors.${1:field}[0]', false) }}",
            "    </p>",
            "</div>",
        ]
    },

    "Bulma Card": {
        "prefix": "bulma-card",
        "body": [
            "<div class=\"card\">",
            "    <div class=\"card-header\">",
            "        <h1 class=\"card-header-title\">",
            "            <span class=\"icon\">",
            "                <i class=\"fa fa-${2:icon}\"></i>",
            "            </span>",
            "            ${1:Card Title}",
            "        </h1>",
            "    </div>",
            "    <div class=\"card-content\">",
            "       ${3:Card Content}",
            "    </div>",
            "</div>",
        ]
    },

    "Bulma Navbar Item": {
        "prefix": "bulma-navbar-item",
        "body": [
            "<a class=\"navbar-item\">",
            "    <span class=\"icon\">",
            "        <i class=\"fa fa-${1:icon}\"></i>",
            "    </span>",
            "    <span>",
            "        ${2:content}",
            "    </span>",
            "</a>",
        ]
    },

    "Bulma Navbar": {
        "prefix": "bulma-navbar",
        "body": [
            "<nav class=\"navbar has-shadow is-spaced\" role=\"navigation\" aria-label=\"main navigation\">",
            "    <div class=\"container\">",
            "        <div class=\"navbar-brand\">",
            "            <a role=\"button\" class=\"navbar-burger burger\" aria-label=\"menu\" aria-expanded=\"false\" data-target=\"navbar\">",
            "                <span aria-hidden=\"true\"></span>",
            "                <span aria-hidden=\"true\"></span>",
            "                <span aria-hidden=\"true\"></span>",
            "            </a>",
            "        </div>",
            "        ",
            "        <div id=\"navbar\" class=\"navbar-menu\">",
            "            <div class=\"navbar-start\">",
            "                ",
            "            </div>",
            "            ",
            "            <div class=\"navbar-end\">",
            "                <div class=\"navbar-item\">",
            "                    <div class=\"buttons\">",
            "                    </div>",
            "                </div>",
            "            </div>",
            "        </div>",
            "    </div>",
            "</nav>",
        ]
    },

    "Bootstrap Form Group Input": {
        "prefix": "vue-bs-input",
        "body": [
            "<div class='form-group'>",
            "    <label for='${1:name}'> ${2:label}: </label>",
            "    <input",
            "        v-model='${1:name}'",
            "        class='form-control'",
            "        :class=\"{'is-invalid': get(this.error_data, 'errors.${1:name}[0]', false)}\"",
            "        type='text'",
            "        id='${1:name}'",
            "        placeholder='${2:label}'>",
            "    <div class='invalid-feedback'>",
            "         {{ get(this.error_data, 'errors.${1:name}[0]', false) }}",
            "    </div>",
            "</div>",
        ]
    },

    "Bootstrap Form Group Textarea": {
        "prefix": "vue-bs-textarea",
        "body": [
            "<div class='form-group'>",
            "    <label for='${1:name}'> ${2:label}: </label>",
            "    <textarea",
            "        v-model='${1:name}'",
            "        class='form-control'",
            "        :class=\"{'is-invalid': get(this.error_data, 'errors.${1:name}[0]', false)}\"",
            "        type='text' id='${1:name}' placeholder='${2:label}'></textarea>",
            "    <div class='invalid-feedback'>{{ get(this.error_data, 'errors.${1:name}[0]', false) }}</div>",
            "</div>",
        ]
    }
}