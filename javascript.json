{
    "Axios POST Request": {
        "prefix": "axios_post",
        "body": [
            "axios.post(${1:url}, ${2:data})",
            "   .then(response => {",
            "       ${3}",
            "   })",
            "   .catch(error => {",
            "       ${4:this.error_data = error.response.data}",
            "   })",
        ]
    },

    "Axios POST Request (Multipart/Form-Data)": {
        "prefix": "axios_post_multipart",
        "body": [
            "axios.post(${1:url}, ${2:data}, { headers: { 'Content-Type': 'multipart/form-data' } })",
            "   .then(response => {",
            "       ${3}",
            "   })",
            "   .catch(error => {",
            "       ${4:this.error_data = error.response.data}",
            "   })",
        ]
    }
}